<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Toplevel" id="toplevel3">
    <property name="background">#690f96</property>
    <property name="geometry">800x600</property>
    <property name="height">200</property>
    <property name="minsize">800|600</property>
    <property name="width">200</property>
    <child>
      <object class="tk.Frame" id="Main">
        <property name="background">#00bfb3</property>
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="anchor">center</property>
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Frame" id="Header">
            <property name="background">#049a8f</property>
            <property name="height">80</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="anchor">w</property>
              <property name="fill">x</property>
              <property name="side">top</property>
            </layout>
            <containerlayout manager="pack">
              <property name="propagate">False</property>
            </containerlayout>
            <child>
              <object class="ttk.Label" id="label3">
                <property name="anchor">s</property>
                <property name="background">#049a8f</property>
                <property name="font">{Yu Mincho} 20 {bold}</property>
                <property name="foreground">#023436</property>
                <property name="text" translatable="yes">Soundpad - demo</property>
                <layout manager="pack">
                  <property name="anchor">w</property>
                  <property name="padx">15</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Separator" id="separator7">
                <property name="orient">horizontal</property>
                <layout manager="pack">
                  <property name="fill">x</property>
                  <property name="side">bottom</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="credits">
                <property name="background">#049a8f</property>
                <property name="font">{Yu Mincho} 8 {italic}</property>
                <property name="foreground">#023436</property>
                <property name="text" translatable="yes">Made by Claudiu Padure</property>
                <layout manager="pack">
                  <property name="anchor">s</property>
                  <property name="side">right</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="SideMenu">
            <property name="background">#03b5aa</property>
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="anchor">w</property>
              <property name="fill">y</property>
              <property name="side">left</property>
            </layout>
            <child>
              <object class="ttk.Label" id="inputMicro">
                <property name="background">#03b5aa</property>
                <property name="font">{Yu Mincho} 11 {bold}</property>
                <property name="foreground">#023436</property>
                <property name="text" translatable="yes">Input MIC:</property>
                <layout manager="pack">
                  <property name="padx">10</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Combobox" id="inputMic">
                <layout manager="pack">
                  <property name="padx">25</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="outputMic">
                <property name="background">#03b5aa</property>
                <property name="font">{Yu Mincho} 11 {bold}</property>
                <property name="foreground">#023436</property>
                <property name="text" translatable="yes">Output MIC:</property>
                <layout manager="pack">
                  <property name="anchor">n</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Combobox" id="OutputMic">
                <layout manager="pack">
                  <property name="anchor">n</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="label5">
                <property name="background">#03b5aa</property>
                <property name="font">{Yu Mincho} 10 {}</property>
                <property name="text" translatable="yes">Version 0.0.1</property>
                <layout manager="pack">
                  <property name="side">bottom</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="label8">
                <property name="background">#03b5aa</property>
                <property name="font">{Yu Mincho} 12 {bold}</property>
                <property name="foreground">#023436</property>
                <property name="text" translatable="yes">Add Sound</property>
                <layout manager="pack">
                  <property name="pady">20</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="pygubu.builder.widgets.pathchooserinput" id="soundPath">
                <property name="type">file</property>
                <layout manager="pack">
                  <property name="anchor">n</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="soundName">
                <property name="font">{Yu Mincho} 12 {}</property>
                <property name="text" translatable="yes">Sound Name</property>
                <layout manager="pack">
                  <property name="pady">10</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="AddButton">
                <property name="default">normal</property>
                <property name="text" translatable="yes">Add</property>
                <layout manager="pack">
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Combobox" id="selectKeybind">
                <layout manager="pack">
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="MicVolume">
                <property name="background">#03b5aa</property>
                <property name="font">{Yu Mincho} 12 {bold}</property>
                <property name="foreground">#023436</property>
                <property name="text" translatable="yes">Microphone volume</property>
                <layout manager="pack">
                  <property name="pady">10</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Scale" id="MicVolumeScale">
                <property name="activebackground">#023436</property>
                <property name="background">#03b5aa</property>
                <property name="borderwidth">5</property>
                <property name="font">{Yu Mincho} 12 {}</property>
                <property name="foreground">#023436</property>
                <property name="highlightbackground">#03b5aa</property>
                <property name="highlightcolor">#023436</property>
                <property name="orient">horizontal</property>
                <property name="relief">raised</property>
                <property name="troughcolor">#049a8f</property>
                <layout manager="pack">
                  <property name="ipadx">25</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="micVolumeSave">
                <property name="font">{Yu Mincho} 11 {}</property>
                <property name="text" translatable="yes">Save</property>
                <layout manager="pack">
                  <property name="pady">5</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Label" id="label1">
                <property name="background">#03b5aa</property>
                <property name="font">{Yu Mincho} 12 {bold}</property>
                <property name="foreground">#023436</property>
                <property name="text" translatable="yes">Mute MIC keybind</property>
                <layout manager="pack">
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Combobox" id="MuteKeybind">
                <layout manager="pack">
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="SaveMute">
                <property name="font">{Yu Mincho} 11 {}</property>
                <property name="text" translatable="yes">Save</property>
                <layout manager="pack">
                  <property name="pady">5</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Separator" id="separator1">
            <property name="orient">vertical</property>
            <property name="takefocus">false</property>
            <layout manager="pack">
              <property name="fill">both</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Separator" id="separator4">
            <property name="orient">vertical</property>
            <layout manager="pack">
              <property name="fill">y</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="sounds">
            <property name="background">#037971</property>
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="anchor">center</property>
              <property name="expand">true</property>
              <property name="fill">both</property>
              <property name="side">bottom</property>
            </layout>
            <child>
              <object class="pygubu.builder.widgets.tkscrolledframe" id="soundZone">
                <property name="background">#037971</property>
                <property name="scrolltype">vertical</property>
                <property name="usemousewheel">true</property>
                <layout manager="pack">
                  <property name="expand">true</property>
                  <property name="fill">both</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="tk.Frame" id="SoundHolder">
                    <property name="background">#03b5aa</property>
                    <property name="height">25</property>
                    <property name="width">200</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="pady">5</property>
                      <property name="side">top</property>
                    </layout>
                    <containerlayout manager="pack">
                      <property name="propagate">False</property>
                    </containerlayout>
                    <child>
                      <object class="ttk.Label" id="SoundName">
                        <property name="background">#03b5aa</property>
                        <property name="font">{Yu Mincho} 12 {bold}</property>
                        <property name="text" translatable="yes">Name Test</property>
                        <layout manager="pack">
                          <property name="padx">10</property>
                          <property name="side">left</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Label" id="keybindText">
                        <property name="background">#03b5aa</property>
                        <property name="font">{Yu Mincho} 9 {bold}</property>
                        <property name="text" translatable="yes">NUM 0</property>
                        <layout manager="pack">
                          <property name="padx">20</property>
                          <property name="side">left</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="playSound">
                        <property name="text" translatable="yes">Play Sound</property>
                        <layout manager="pack">
                          <property name="padx">10</property>
                          <property name="side">right</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Button" id="delete">
                        <property name="text" translatable="yes">X</property>
                        <layout manager="pack">
                          <property name="side">right</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="ttk.Scale" id="scale1">
                        <property name="orient">horizontal</property>
                        <layout manager="pack">
                          <property name="side">top</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
